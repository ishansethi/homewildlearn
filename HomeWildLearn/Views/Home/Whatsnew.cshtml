<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">
                Happenings
                <small>around Australia.</small>
            </h1>
            <ol class="breadcrumb">
                <li>
                    @Html.ActionLink("Home", "Index", "Home")
                </li>
                <li class="active">What's New</li>
